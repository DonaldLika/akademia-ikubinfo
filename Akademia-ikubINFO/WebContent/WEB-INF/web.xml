<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>Akademia-ikubINFO</display-name>
 <welcome-file-list>
		<welcome-file>hello-jsp.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>HelloServlet2</servlet-name>
		<servlet-class>al.ikubinfo.akademia.servlets.HelloServlet2</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>HelloServlet2</servlet-name>
		<url-pattern>/helloServlet2</url-pattern>
	</servlet-mapping>

	<filter>
		<filter-name>LoginFilter</filter-name>
		<filter-class>al.ikubinfo.akademia.filters.LoginFilter</filter-class>
		<init-param>
			<param-name>init-param</param-name>
			<param-value>Paramter inicializimi</param-value>
		</init-param>
	</filter>
	<filter-mapping>
		<filter-name>LoginFilter</filter-name>
		<url-pattern>/data/*</url-pattern>
	</filter-mapping>
</web-app>